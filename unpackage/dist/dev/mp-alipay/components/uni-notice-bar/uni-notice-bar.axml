<block a:if="{{show}}"><view data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" class="uni-noticebar" style="{{'background-color:'+(backgroundColor)+';'+('color:'+(color)+';')}}" onTap="__e"><block a:if="{{showClose==='true'||showClose===true}}"><view class="uni-noticebar__close"><uni-icons vue-id="1" type="closefill" size="12" onVueInit="__l"></uni-icons></view></block><view class="{{(('uni-noticebar__content')+' '+((scrollable||single||moreText)?'uni-noticebar--flex':''))}}"><block a:if="{{showIcon==='true'||showIcon===true}}"><view class="uni-noticebar__content-icon" style="{{'background-color:'+(backgroundColor)+';'+('color:'+(color)+';')}}"><uni-icons vue-id="2" color="{{color}}" type="sound" size="14" onVueInit="__l"></uni-icons></view></block><view class="{{((('uni-noticebar__content-text')+' '+((scrollable)?'uni-noticebar--scrollable':''))+' '+((!scrollable&&(single||moreText))?'uni-noticebar--single':''))}}"><view class="uni-noticebar__content-inner" style="{{'animation:'+(animation)+';'+('-webkit-animation:'+(animation)+';')}}" id="{{elId}}">{{text}}</view></view><block a:if="{{showGetMore==='true'||showGetMore===true}}"><view data-event-opts="{{[['tap',[['clickMore',['$event']]]]]}}" class="uni-noticebar__content-more" style="{{'width:'+(moreText?'180upx':'20px')+';'}}" onTap="__e"><block a:if="{{moreText}}"><view class="uni-noticebar__content-more-text">{{moreText}}</view></block><uni-icons vue-id="3" type="arrowright" size="14" onVueInit="__l"></uni-icons></view></block></view></view></block>