<view style="background:#EEEEEE;width:100%;height:100vh;"><block a:if="{{user.account}}"><view><view class="uni-list"><view class="uni-list-cell u_f_jc u_f_ac" style="padding:20rpx 0;"><view class="userImg"><image src="../../static/images/homeed.png" mode="widthFix"></image></view></view><view class="uni-list-cell"><view class="uni-list-cell-navigate">{{'账号:'+user.account+''}}</view></view><view data-event-opts="{{[['tap',[['reviseUser',['$event']]]]]}}" class="uni-list-cell" onTap="__e"><view class="uni-list-cell-navigate uni-navigate-right">{{'姓名:'+user.realName+''}}</view></view><view data-event-opts="{{[['tap',[['reviseUser',['$event']]]]]}}" class="uni-list-cell" onTap="__e"><view class="uni-list-cell-navigate uni-navigate-right">{{'单位:'+user.unit+''}}</view></view><view data-event-opts="{{[['tap',[['reviseUser',['$event']]]]]}}" class="uni-list-cell" onTap="__e"><view class="uni-list-cell-navigate uni-navigate-right">{{'紧急联系电话:'+user.telephone+''}}</view></view><view data-event-opts="{{[['tap',[['reviseUser',['$event']]]]]}}" class="uni-list-cell" onTap="__e"><view class="uni-list-cell-navigate uni-navigate-right">{{'微信:'+user.weixin+''}}</view></view><view data-event-opts="{{[['tap',[['reviseUser',['$event']]]]]}}" class="uni-list-cell" onTap="__e"><view class="uni-list-cell-navigate uni-navigate-right">{{'QQ:'+user.tenxun+''}}</view></view><view data-event-opts="{{[['tap',[['reviseUser',['$event']]]]]}}" class="uni-list-cell" onTap="__e"><view class="uni-list-cell-navigate uni-navigate-right">{{'情况:'+user.picture+''}}</view></view><view data-event-opts="{{[['tap',[['revisePwd',['$event']]]]]}}" class="uni-list-cell" onTap="__e"><view class="uni-list-cell-navigate uni-navigate-right">修改密码</view></view></view></view></block><block a:else><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="user u_f_jfs u_f_ac" onTap="__e"><view class="userImg"><image src="../../static/images/homeed.png" mode="widthFix"></image></view><view class="userInfo login">Hi,您还未登录</view></view></block><block a:if="{{user.account}}"><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" onTap="__e">退出登录</button></view></block></view>